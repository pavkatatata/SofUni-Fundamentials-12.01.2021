using System;
using System.Collections.Generic;
using System.Linq;
using System.Numerics;

namespace ConsoleApp6
{
    class Program
    {
        static void Main(string[] args)
        {

            int n = int.Parse(Console.ReadLine());

            BigInteger num = 1;

            for (int i = 2; i <= n; i++)
            {
                num = num * i;
               
            }
            Console.Write(num);
        }

    }
}
