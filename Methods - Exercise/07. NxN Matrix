using System;
using System.Runtime.InteropServices.WindowsRuntime;

namespace ConsoleApp31
{
    class Program
    {
        static void Main(string[] args)
        {

            int n = int.Parse(Console.ReadLine());

            NxNmatrix(n);





        }

        private static void NxNmatrix(int n)
        {

            for (int i = 0; i < n; i++)
            {
                
                for (int j = 0; j < n; j++)
                {
                    Console.Write($"{n} ");
                    
                }
                Console.WriteLine();
            }

          

        }
    }
}
