using System;
using System.Linq;

namespace ConsoleApp17
{
    class Program
    {
        static void Main(string[] args)
        {

            int n = int.Parse(Console.ReadLine());
            int[] train = new int[n];
            int wagon = 0;

            int sum = 0;

            for (int i = 0; i < train.Length; i++)
            {
                wagon = int.Parse(Console.ReadLine());
                train[i] = wagon;
                sum += train[i];


            }

            Console.Write(String.Join(" ",train));
            Console.WriteLine();
            Console.Write(sum);


        }
    }
}
