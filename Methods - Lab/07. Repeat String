using System;
using System.Runtime.ExceptionServices;

namespace ConsoleApp27
{
    class Program
    {
        static void Main(string[] args)
        {

            string input = Console.ReadLine();
            int n = int.Parse(Console.ReadLine());

            RpeatString(input, n);


        }

        private static void RpeatString(string input, int n)
        {

            for (int j = 0; j < n; j++)
            {
                Console.Write(input);
            }
        }
    }

}
